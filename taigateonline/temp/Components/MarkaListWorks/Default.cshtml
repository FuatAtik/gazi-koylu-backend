                        <div class="project-carousel-go"><i class="icon ion-ios-arrow-right"></i></div>
                        <div class="project-carousel-category ">@Model.RedirectText</div>
                      </div>
                    </a>
                  </div>
                  <div class="grid-item col-sm-6">
                    <a href="@Model.RedirectUrl3" class="project-item brand-image-3" style="background-image:url(@Model.ImageBgUrl3);">
                      <div class="project-carousel-photo"></div>
