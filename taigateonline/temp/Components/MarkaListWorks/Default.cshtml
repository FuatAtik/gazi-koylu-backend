@model Taigate.Data.Data.Entities.Components.MarkaListWorks

 <div class="section text-left sector-list" id="works">
              <div class="container">
                <div class="project-list-flash row no-gutters a-grid">
                  <div class="grid-item col-sm-6">
                    <div class="project-list-title">
                      <h3 class="fs14 text-white">@Model.Text1</h3>
                      <h2 class="fs36 text-white mt30"> <span class="font-ubuntu-bold">@Model.Text2</span>  <br> <span class="font-ubuntu-light">@Model.Text3</span> </h2>
                    </div>
                  </div>
                  <div class="grid-item col-sm-6">
                    <a href="@Model.RedirectUrl1" class="project-item brand-image-1" style="background-image:url(@Model.ImageBgUrl1);">
                      <div class="project-carousel-photo"></div>
                      <div class="project-carousel-detail">
                        <h2 class="project-carousel-title">@Model.WorkTitle1</h2>
                        <div class="project-carousel-go"><i class="icon ion-ios-arrow-right"></i></div>
                        <div class="project-carousel-category">@Model.RedirectText </div>
                      </div>
                    </a>
                  </div>
                  <div class="grid-item col-sm-6">
                    <a href="@Model.RedirectUrl2" class="project-item brand-image-2" style="background-image:url(@Model.ImageBgUrl2);">
                      <div class="project-carousel-photo"></div>
