@model List<Taigate.Data.Data.Entities.Components.SosyalSorumlulukProje>

<div class="social-responsibility">
    <div class="container text-center">
        <div class="mt50 mb60">
            <h3 class="fs30 mt90 wow fadeInUp contact-title  text-blue" data-wow-delay=".6s">
                Ülkemizin “yaşam kalitesini iyileştirme” amacına ulaşmak için ekonomik gelişmeye
                destek verme sorumlululuğu ile sosyal sorumluluk projelerimizi hayata geçiriyoruz.
            </h3>
        </div>
        <div class="project-list-listing a-grid">
            @foreach (var item in Model)
            {
                <div class="grid-item category-branding category-mobile">
                    <a href="">
                        <div class="project-item">
                            <div class="project-listing-photo" style="background-image:url(@item.CoverImage)"></div>
                        </div>
                        <div class="project-listing-detail">
                            <h2 class="project-listing-title">@item.Title</h2>
                            <div class="project-listing-category">Detaylı İnceleyin</div>
                        </div>
                    </a>
                </div>
            }
        </div>

        <div class="product-detail-share">
            <div class="text-grey fs20">DOĞANLAR <br> YATIRIM HOLDİNG</div>
