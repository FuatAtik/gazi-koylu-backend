@model List<Taigate.Data.Data.Entities.Components.OperasyonHaritasiUlkelerArea>

<div class="col-lg-7 col-sm-8">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-4">
                <div class="project-info-item wow fadeInUp " data-wow-delay=".2s" data-wow-duration=".5s" data-wow-offset="50">
                    <div class="project-info-title">@item.Title</div>
                    <div class="project-info-descr">@item.Description1</div>
                    <div class="project-info-descr">@item.Description2</div>
                </div>
            </div>
        }
        
    </div>
</div>

